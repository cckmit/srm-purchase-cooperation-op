<mapper namespace="org.srm.purchasecooperation.cux.sinv.infra.mapper.SinvRcvTrxWaitingAsnStatusMapper">


    <select id="selectRcvTrxWaitingAsnStatus" resultType="java.lang.String">
    select sah.asn_status
	from sinv_asn_header sah
	where sah.asn_num = #{asnNum}
	and sah.tenant_id =#{tenantId};

    </select>
</mapper>
